-- Entrada (igual antes)
local scale = Instance.new("UIScale")
scale.Scale = 0.88
scale.Parent = main

tween(dim, TweenInfo.new(0.35, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {BackgroundTransparency = 0.35})
tween(blur, TweenInfo.new(0.35, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Size = 12})
tween(main, TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {GroupTransparency = 0})
tween(stroke, TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Transparency = 0})
tween(scale, TweenInfo.new(0.35, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Scale = 1})

-- Saída (fade-out tudo junto) + trava
local closing = false
local function closeFade()
	if closing then return end
	closing = true

	close.Active = false
	close.AutoButtonColor = false

	local outInfo = TweenInfo.new(0.22, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
	tween(dim, outInfo, {BackgroundTransparency = 1})
	tween(blur, outInfo, {Size = 0})
	tween(main, outInfo, {GroupTransparency = 1})
	tween(stroke, outInfo, {Transparency = 1})
	tween(scale, outInfo, {Scale = 0.92})

	task.delay(0.24, function()
		if blur then blur:Destroy() end
		if gui then gui:Destroy() end
	end)
end

close.MouseButton1Click:Connect(closeFade)

-- Ativa automaticamente após 10s (uma vez) e fecha
task.delay(5, function()
	if activated or closing then return end
	onAutoContinue()
	closeFade()
end)
