local Tab =
Window:NewTab("Musics")
local Section = Tab:NewSection("Game")

Section:NewButton("Sounds Support (click me)", ".", function()
-- LocalScript

local ignoredIds = {
	["rbxassetid://9043578323"] = true,
	["rbxassetid://110733765539890"] = true,
	["rbxassetid://124249063943651"] = true,
}

-- Fun√ß√£o que para sons de m√∫sica, exceto os ignorados
local function stopMusicSounds()
	for _, sound in ipairs(game:GetDescendants()) do
		if sound:IsA("Sound") and sound.IsPlaying then
			local nameLower = string.lower(sound.Name)
			local isMusic = sound.Looped or sound.Volume >= 1 or nameLower:find("music") or nameLower:find("bgm")

			local isIgnored = ignoredIds[sound.SoundId] == true

			if isMusic and not isIgnored then
				sound:Stop()
			end
		end
	end
end

-- Loop cont√≠nuo
while true do
	stopMusicSounds()
	wait(1) -- Verifica a cada 1 segundo
end
    print("Clicked")
end)

Section:NewButton("Five Nights At Freddy", ".", function()
-- LocalScript (coloque em StarterPlayerScripts ou StarterGui)

local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Criar a GUI
local screenGui = Instance.new("ScreenGui")
screenGui.IgnoreGuiInset = true
screenGui.ResetOnSpawn = false
screenGui.Parent = playerGui

-- Tela preta
local blackFrame = Instance.new("Frame")
blackFrame.Size = UDim2.new(1, 0, 1, 0)
blackFrame.Position = UDim2.new(0, 0, 0, 0)
blackFrame.BackgroundColor3 = Color3.new(0, 0, 0)
blackFrame.BackgroundTransparency = 0
blackFrame.Parent = screenGui

-- Imagem
local imageLabel = Instance.new("ImageLabel")
imageLabel.Size = UDim2.new(0.98, 0, 0.98, 0)
imageLabel.Position = UDim2.new(0.01, 0, 0.01, 0)
imageLabel.BackgroundTransparency = 1
imageLabel.Image = "rbxassetid://15882055952" -- Substitua se quiser outra imagem
imageLabel.ScaleType = Enum.ScaleType.Fit
imageLabel.ImageTransparency = 1
imageLabel.Parent = blackFrame

-- Som
local sound = Instance.new("Sound")
sound.SoundId = "rbxassetid://110733765539890"
sound.Volume = 10
sound.Looped = false
sound.Parent = workspace
sound:Play()

-- Fade-in
for i = 1, 10 do
	imageLabel.ImageTransparency = 1 - (i * 0.1)
	wait(0.05)
end

-- Espera 3 segundos vis√≠vel
wait(3)

-- Fade-out
for i = 1, 10 do
	imageLabel.ImageTransparency = i * 0.1
	wait(0.05)
end

-- Limpa GUI
screenGui:Destroy()
    print("Clicked")
end)

