ScreenGui.Name = "ScreenGui"
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ResetOnSpawn = false

local Toggle = Instance.new("ImageButton")
Toggle.Name = "Toggle"
Toggle.Parent = ScreenGui
Toggle.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Toggle.Position = UDim2.new(0, 10, 0, 10) -- canto superior esquerdo com uma margem
Toggle.Size = UDim2.new(0, 40, 0, 40) -- quadrado com tamanho controlado
Toggle.Image = "rbxassetid://12466484955"
Toggle.Draggable = true
Toggle.Active = true -- necess√°rio para funcionar o Draggable
Toggle.Selectable = true

-- Bordas arredondadas
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 8)
corner.Parent = Toggle

Toggle.MouseButton1Click:Connect(function()
    Library:ToggleUI()
end)
