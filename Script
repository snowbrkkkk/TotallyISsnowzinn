Tab:AddButton({
	Name = "Auto get Stands",
	Callback = function()
local player = game.Players.LocalPlayer

-- GUI principal
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "SnowStandFarmUI"
screenGui.ResetOnSpawn = false
screenGui.Parent = player:WaitForChild("PlayerGui")

-- Frame central
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 350, 0, 400)
frame.Position = UDim2.new(0.5, -175, 0.5, -200)
frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
frame.BorderSizePixel = 0
frame.Visible = true
frame.Parent = screenGui

-- Título
local title = Instance.new("TextLabel")
title.Size = UDim2.new(1, 0, 0, 50)
title.Position = UDim2.new(0, 0, 0, 0)
title.BackgroundTransparency = 1
title.Text = "Snow Stand Farm"
title.TextColor3 = Color3.new(1, 1, 1)
title.TextSize = 28
title.Font = Enum.Font.SourceSansBold
title.Parent = frame

-- Botão Cube
local cubeButton = Instance.new("TextButton")
cubeButton.Size = UDim2.new(1, -20, 0, 40)
cubeButton.Position = UDim2.new(0, 10, 0, 60)
cubeButton.Text = "Cube (No Ban Risk)"
cubeButton.BackgroundColor3 = Color3.fromRGB(70, 130, 180)
cubeButton.TextColor3 = Color3.new(1, 1, 1)
cubeButton.Font = Enum.Font.SourceSansBold
cubeButton.TextSize = 20
cubeButton.Parent = frame
cubeButton.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet("https://pastebin.com/raw/RAb6ExHN"))()
end)

-- Botão "Mostrar Stands"
local showStandsBtn = Instance.new("TextButton")
showStandsBtn.Size = UDim2.new(1, -20, 0, 40)
showStandsBtn.Position = UDim2.new(0, 10, 0, 110)
showStandsBtn.Text = "Mostrar Stands"
showStandsBtn.BackgroundColor3 = Color3.fromRGB(70, 130, 180)
showStandsBtn.TextColor3 = Color3.new(1, 1, 1)
showStandsBtn.Font = Enum.Font.SourceSansBold
showStandsBtn.TextSize = 20
showStandsBtn.Parent = frame

-- ScrollingFrame para Stands
local scrollFrame = Instance.new("ScrollingFrame")
scrollFrame.Size = UDim2.new(1, -20, 1, -170)
scrollFrame.Position = UDim2.new(0, 10, 0, 160)
scrollFrame.CanvasSize = UDim2.new(0, 0, 0, 0)
scrollFrame.ScrollBarThickness = 6
scrollFrame.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
scrollFrame.BorderSizePixel = 0
scrollFrame.Visible = false
scrollFrame.Parent = frame

local layout = Instance.new("UIListLayout", scrollFrame)
layout.Padding = UDim.new(0, 5)
layout.SortOrder = Enum.SortOrder.LayoutOrder

-- Lista de Stands e scripts
local stands = {
    {name = "OMT", url = "https://pastebin.com/raw/G93sch6q"},
    {name = "TheWorld", url = "https://pastebin.com/raw/MW2g3EM6"},
    {name = "DoppioKingCrimson", url = "https://pastebin.com/raw/r5sRitc1"},
    {name = "WhitesnakeAU", url = "https://pastebin.com/raw/bMCgMgZ0"},
    {name = "TheWorldOVA", url = "https://pastebin.com/raw/C9quXvM8"}
}

-- Cria botões para cada Stand
for _, stand in ipairs(stands) do
    local button = Instance.new("TextButton")
    button.Size = UDim2.new(1, 0, 0, 30)
    button.Text = stand.name
    button.BackgroundColor3 = Color3.fromRGB(70, 130, 180)
    button.TextColor3 = Color3.new(1, 1, 1)
    button.Font = Enum.Font.SourceSansBold
    button.TextSize = 16
    button.Parent = scrollFrame
    button.MouseButton1Click:Connect(function()
        loadstring(game:HttpGet(stand.url))()
    end)
end

-- Atualiza tamanho do scroll automático
scrollFrame.CanvasSize = UDim2.new(0, 0, 0, layout.AbsoluteContentSize.Y)
layout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
    scrollFrame.CanvasSize = UDim2.new(0, 0, 0, layout.AbsoluteContentSize.Y)
end)

-- Alterna visibilidade do scroll de Stands
local standsListOpen = false
showStandsBtn.MouseButton1Click:Connect(function()
    standsListOpen = not standsListOpen
    scrollFrame.Visible = standsListOpen
end)

-- Botão preto flutuante para abrir/fechar a GUI
local toggleButton = Instance.new("TextButton")
toggleButton.Size = UDim2.new(0, 100, 0, 35)
toggleButton.Position = UDim2.new(0, 10, 0.5, -150)
toggleButton.Text = "Fechar GUI"
toggleButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0) -- Preto
toggleButton.TextColor3 = Color3.new(1, 1, 1)
toggleButton.Font = Enum.Font.SourceSansBold
toggleButton.TextSize = 16
toggleButton.Parent = screenGui

-- Alternar visibilidade da GUI principal
local guiVisible = true
toggleButton.MouseButton1Click:Connect(function()
    guiVisible = not guiVisible
    frame.Visible = guiVisible
    toggleButton.Text = guiVisible and "Fechar GUI" or "Abrir GUI"
end)
      		print("button pressed")
  	end    
})

